+ 		print(CIs.noswim)
+ 	cat("\n-------------------------------------------")
+ 	cat("\nSWIMMERS -- Cases (n) and Exposed (N)")
+ 	cat("\nOverall and stratified by marine/freshwater")
+ 	cat("\n-------------------------------------------\n")
+ 		print(Ns)
+ 	cat("\n-------------------------------------------")
+ 	cat("\nSWIMMERS -- Cumulative Incidence (95% CIs)")
+ 	cat("\nOverall and stratified by marine/freshwater")
+ 	cat("\n-------------------------------------------\n")
+ 		print(CIs)
+ 	cat("\n----------------------------------------------")
+ 	cat("\nAdjusted Cumulative Incidence Ratios (95% CIs)")
+ 	cat("\nOverall and stratified by marine/freshwater")
+ 	cat("\n----------------------------------------------\n")
+ 		print(CIRs)
+ 		
+ 	# --------------------------------------
